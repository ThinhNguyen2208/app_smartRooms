{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/ScreenWrapper.jsx",
    "constants/index.js",
    "components/TabBarButton.jsx",
    "components/TabBar.jsx",
    "app/(tabs)/notification.jsx",
    "app/(tabs)/_layout.jsx",
    "components/RoomDetailInfo.jsx"
  ]
}